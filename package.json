    "eslint-plugin-angular": "^4.0.1",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^4.3.8",
    "hygen": "^5.0.3",
    "jest": "~24.9.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "sass": "^1.34.0",
    "vite": "^2.3.4"
